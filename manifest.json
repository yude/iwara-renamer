{
    "manifest_version": 2,
    "name": "Iwara Renamer",
    "version": "1.0.0",
    "description": "Rename file from iwara.tv automatically",
    "permissions": [
        "downloads",
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.iwara.tv/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ]
}